{
    "name": "find-my-doctor-backend",
    "version": "1.0.0",
    "description": "Backend for Find My Doctor AI Application",
    "main": "Server.js",
    "type": "module",
    "scripts": {
        "dev": "nodemon --watch src --exec tsx src/Server.ts",
        "start": "tsx src/Server.ts",
        "inngest:dev": "npx inngest-cli@latest dev -u http://localhost:5001/api/inngest",
        "format": "prettier . --write",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prepare": "husky install",
        "test": "jest --watch --runInBand"
    },
    "author": "Shaikh Sajed <shaikhsajedcoolpop@gmail.com>",
    "license": "ISC",
    "devDependencies": {
        "@types/express": "^4.17.23",
        "@types/jest": "^29.5.5",
        "@types/node": "^20.7.1",
        "@typescript-eslint/eslint-plugin": "^6.7.3",
        "@typescript-eslint/parser": "^6.7.3",
        "eslint": "^8.50.0",
        "eslint-config-prettier": "^9.0.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "lint-staged": "^14.0.1",
        "prettier": "3.0.3",
        "ts-jest": "^29.1.1",
        "typescript": "^5.2.2"
    },
    "lint-staged": {
        "*.ts": [
            "npm run lint",
            "npm run format"
        ]
    },
    "dependencies": {
        "@inngest/agent-kit": "^0.8.0",
        "@pinecone-database/pinecone": "^6.1.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/cookie-parser": "^1.4.9",
        "@types/cors": "^2.8.19",
        "@types/http-errors": "^2.0.2",
        "@types/supertest": "^2.0.13",
        "@types/winston": "^2.4.4",
        "axios": "^1.9.0",
        "bcrypt": "^6.0.0",
        "bcryptjs": "^3.0.2",
        "cohere-ai": "^7.17.1",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^4.18.2",
        "express-jwt": "^8.5.1",
        "express-validator": "^7.2.1",
        "http-errors": "^2.0.0",
        "inngest": "^3.39.1",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.15.1",
        "nodemon": "^3.0.1",
        "openai": "^5.2.0",
        "supertest": "^6.3.3",
        "ts-node": "^10.9.1",
        "tsx": "^4.20.3",
        "winston": "^3.10.0"
    }
}
